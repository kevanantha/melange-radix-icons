{
  "name": "melange-radix-icons",
  "version": "0.0.2",
  "description": "Melange bindings for Radix Icons",
  "scripts": {
    "dune.watch": "opam exec -- dune build -w",
    "dune.build": "opam exec -- dune build",
    "dune.sync": "opam exec -- dune build melange-radix-icons.opam && opam exec -- dune build @install",
    "opam.update": "opam update",
    "opam.install": "opam install . --deps-only",
    "opam.publish": "opam publish"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@radix-ui/react-icons": "^1.3.0"
  },
  "keywords": [ "melange", "radix", "icons" ],
  "author": "I Putu Kevin Anantha Vijaya",
  "license": "MIT"
}
